                                    <my:CoreTreeViewItem  Header="10) OMA DEPT - Maintenance" ToolTip="OMA DEPT - Maintenance" Tag = "m040_dtl" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
