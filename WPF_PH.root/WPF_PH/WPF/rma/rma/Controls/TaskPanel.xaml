                                    <my:CoreTreeViewItem  Header="07) 'NORMAL' DOCTOR MASTER Maintenance" Mode="F" ToolTip="'NORMAL' DOCTOR MASTER Maintenance" Tag = "m020" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="08) Special Payments Entry/Query" ToolTip="Special Payments Entry/Query" Tag = "D114" Mode="F" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
