                                    <my:CoreTreeViewItem  Header="E) Contract Code Master" ToolTip="Contract Code Master" Tag = "m101" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="F) Client Doctor Master" ToolTip="Client Doctor Master" Tag = "m102" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="I) Referring Physician Inquiry" ToolTip="Referring Physician Inquiry" Tag = "in01" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="J) Social Contract Factor Ranges" ToolTip="Social Contract Factor Ranges" Tag = "m940" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>

                                    <my:CoreTreeViewItem  Header="OHIP Manual Rejections" ToolTip="OHIP Manual Rejections" Cursor = "Hand"  Selected="TreeViewItem_Selected"  Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}">
                                        <my:CoreTreeViewItem  Header="K) Message Maintenance" ToolTip="Message Maintenance" Tag = "m093" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                        <my:CoreTreeViewItem  Header="L) Message Categories" ToolTip="Message Categories" Tag = "m092" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                        <!--<my:CoreTreeViewItem  Header="" ToolTip="" Tag = "m090g" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>-->
                                        <my:CoreTreeViewItem  Header="N) On-line User Identification" ToolTip="On-line User Identification" Tag = "m083" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                        <my:CoreTreeViewItem  Header="O) Transfer Claim between Patients" ToolTip="Transfer Claim between Patients" Tag = "utl1002" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                        <my:CoreTreeViewItem  Header="P) Proposed MOH Prices - Maintenance" ToolTip="Proposed MOH Prices - Maintenance" Tag = "m941" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                        <my:CoreTreeViewItem  Header="Q) Doctor Revenue Translation Table" ToolTip="Doctor Revenue Translation Table" Tag = "m923" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                        <my:CoreTreeViewItem  Header="R) Non Fee for Service Location Table" ToolTip="Non Fee for Service Location Table" Tag = "m924" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
