                                    <my:CoreTreeViewItem  Header="" ToolTip="" Tag = "" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>

