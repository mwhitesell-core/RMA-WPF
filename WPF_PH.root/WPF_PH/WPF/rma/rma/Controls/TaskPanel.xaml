                                    <my:CoreTreeViewItem  Header="03) PAYROLL CALCULATION Settings - by group/doctor" ToolTip="PAYROLL CALCULATION Settings - by group/doctor" Tag = "M116" Mode="F" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="04) CONSTANTS Master (Current EP, Fiscal Start/End)" ToolTip="CONSTANTS Master (Current EP, Fiscal Start/End)" Tag = "M090F" Mode="F" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="05) Valid P.E.D.s , ACCOUNTING Cycles" ToolTip="Valid P.E.D.s , ACCOUNTING Cycles" Tag = "M191" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
