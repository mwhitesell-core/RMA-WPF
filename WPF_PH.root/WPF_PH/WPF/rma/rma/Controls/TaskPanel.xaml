                                    <my:CoreTreeViewItem  Header="4) Location Master" ToolTip="Location Master" Tag = "m030" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="5) OMA/OHIP Fee Master" ToolTip="OMA/OHIP Fee Master" Tag = "m040" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="6) Department Master" ToolTip="Department Master" Tag = "m070" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="Y) Company Master" ToolTip="Company Master" Tag = "m123" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="7) Bank Master" ToolTip=" Bank Master" Tag = "m080" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="8) Constants Master" ToolTip="Constants Master" Tag = "m090COBOL" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="Z) Constants Master - all PEDs" ToolTip="m090" Tag = "m090" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="9) Equivalence OHIP Code Master" ToolTip="Equivalence OHIP Code Master" Tag = "m098" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
                                    <my:CoreTreeViewItem  Header="V) AFP Groups Sort" ToolTip="AFP Groups Sort" Tag = "m074a" Cursor = "Hand" Selected="TreeViewItem_Selected" Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}" PreviewMouseLeftButtonDown="CoreTreeViewItem_PreviewMouseLeftButtonDown"/>
