                                <my:CoreTreeViewItem  Header="6) Physician Payroll Sub-menu" ToolTip="Physician Payroll Sub-menu" Cursor = "Hand"  Selected="TreeViewItem_Selected"  Style="{DynamicResource TreeViewItemStyle1}" HeaderTemplate = "{DynamicResource HeaderTemplate1}">
