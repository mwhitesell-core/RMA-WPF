<Window x:Class = "rma.Views.AccessDenied" xmlns = "http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x = "http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d = "http://schemas.microsoft.com/expression/blend/2008" xmlns:my = "clr-namespace:rma" ResizeMode = "NoResize" WindowStyle = "ThreeDBorderWindow" ShowInTaskbar = "False" WindowStartupLocation = "CenterOwner" Title = "AccessDenied" Width = "500" Height = "200" Icon = "/rma;component/rma.ico">
	<Grid x:Name = "LayoutRoot">
		<Grid.RowDefinitions>
			<RowDefinition Height = "100"/>
