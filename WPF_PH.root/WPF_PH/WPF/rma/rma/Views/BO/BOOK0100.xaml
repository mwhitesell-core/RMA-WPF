                <myControl:Designer x:Name="dsrDesigner_05"  IsNumberedDesigner="True"></myControl:Designer>
                <myControl:Designer x:Name="dsrDesigner_09"  IsNumberedDesigner="True"></myControl:Designer>
            </StackPanel>
            
            <myControl:TextBlock Margin="23,0,0,0" Text="Year" Grid.Row="5" Grid.Column="2"/>
            <myControl:TextBox Margin="131,0,0,0" x:Name="fldT_YEAR" Grid.Row="5" Grid.Column="2" FieldName="T_YEAR" FieldID="" DataType="Numeric" ShiftType="NotSet" Heading="" LeadingSign="" Fill="" ToolTip="" Float="" TrailingSign="" Required="True" Display="NotSet" Omit="NotSet" NoChange="NotSet" Fixed="NotSet" Duplicate="NotSet" Silent="NotSet"  NoCorrect="NotSet" NoEntry="NotSet" NoSelect="NotSet" PreDisplay="NotSet" Default="" TabIndex="1" Pattern="" Picture="" Significance="0"  Width="45" MaxLength="4"></myControl:TextBox>
