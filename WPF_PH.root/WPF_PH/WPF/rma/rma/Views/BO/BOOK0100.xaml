            <myControl:TextBox Margin="95,0,0,0" x:Name="fldLOCN_BK_COMMENTS_BK_COMMENT_1" Grid.Row="9" Grid.Column="2" FieldName="LOCN_BK_COMMENTS.BK_COMMENT_1" FieldID="Designer_09" DataType="Character" ShiftType="NotSet" Heading="" LeadingSign="" Fill="" ToolTip="" Float="" TrailingSign="" Required="NotSet" Display="ChangeEntryFind" Omit="NotSet" NoChange="NotSet" Fixed="NotSet" Duplicate="NotSet" Silent="NotSet"  NoCorrect="NotSet" NoEntry="NotSet" NoSelect="NotSet" PreDisplay="NotSet" Default="" TabIndex="13" Pattern="" Picture="" Significance="0"  Width="549" MaxLength="60"></myControl:TextBox>
            <myControl:TextBlock Margin="0,0,0,0" Text="Staff" Grid.Row="10" Grid.Column="2"/>
            <myControl:TextBox Margin="95,0,0,0" x:Name="fldLOCN_BK_COMMENTS_BK_COMMENT_2" Grid.Row="10" Grid.Column="2" FieldName="LOCN_BK_COMMENTS.BK_COMMENT_2" FieldID="Designer_09" DataType="Character" ShiftType="NotSet" Heading="" LeadingSign="" Fill="" ToolTip="" Float="" TrailingSign="" Required="NotSet" Display="ChangeEntryFind" Omit="NotSet" NoChange="NotSet" Fixed="NotSet" Duplicate="NotSet" Silent="NotSet"  NoCorrect="NotSet" NoEntry="NotSet" NoSelect="NotSet" PreDisplay="NotSet" Default="" TabIndex="14" Pattern="" Picture="" Significance="0"  Width="549" MaxLength="60"></myControl:TextBox>
