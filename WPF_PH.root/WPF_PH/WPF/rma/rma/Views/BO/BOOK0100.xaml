            <myControl:TextBox Margin="131,0,0,0" x:Name="fldT_CHANGEOVER" Grid.Row="7" Grid.Column="2" FieldName="T_CHANGEOVER" FieldID="" DataType="Character" ShiftType="UpShift" Heading="" LeadingSign="" Fill="" ToolTip="Enter a changeover day code to select just those changeovers" Float="" TrailingSign="" Required="NotSet" Display="NotSet" Omit="NotSet" NoChange="NotSet" Fixed="NotSet" Duplicate="NotSet" Silent="NotSet"  NoCorrect="NotSet" NoEntry="NotSet" NoSelect="NotSet" PreDisplay="NotSet" Default="" TabIndex="9" Pattern="" Picture="" Significance="0"  Width="27" MaxLength="2"></myControl:TextBox>
