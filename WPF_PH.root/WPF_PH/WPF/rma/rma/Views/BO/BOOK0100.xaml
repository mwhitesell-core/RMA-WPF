            <myControl:TextBox Margin="311,0,0,0" x:Name="fldT_PROPERTY_TYPE" Grid.Row="5" Grid.Column="2" FieldName="T_PROPERTY_TYPE" FieldID="" DataType="Character" ShiftType="UpShift" Heading="" LeadingSign="" Fill="" ToolTip="LOCATION(2)+BEDROOMS(1)+TYPE(2)+BATHS(1) : Enter Values or SPACES" Float="" TrailingSign="" Required="NotSet" Display="NotSet" Omit="NotSet" NoChange="NotSet" Fixed="NotSet" Duplicate="NotSet" Silent="NotSet"  NoCorrect="NotSet" NoEntry="NotSet" NoSelect="NotSet" PreDisplay="NotSet" Default="" TabIndex="2" Pattern="" Picture="" Significance="0"  Width="63" MaxLength="6"></myControl:TextBox>
