
exec $obj/r030f1 nogo
and sel if part-hdr-explan-cd = ' ' and &
   (x-part-bal < -5 or x-part-bal > 5)
set rep dev disc name ru030c_blank
set formfeed
go
exec $obj/r030f1 nogo
and sel if part-hdr-explan-cd = 'DG'          
set rep dev disc name ru030c_DG         
set formfeed
go
exec $obj/r030f1 nogo
and sel if part-hdr-explan-cd = 'S7'          
set rep dev disc name ru030c_S7         
set formfeed
go
exit
