element comp-type                        character size 1                    &
  upshift                                                                    &
  values  "I",                                                               &
          "E",                                                               &
          "D",                                                               &
          "C",                                                               &
          "M",                                                               &
          "P",                                                               &
          "A"                                                                &
  default item datatype character size 1                                     &
  help        "Income/Expense/Deduc/Manual paym/earnings Paym/Calc/Advance"  &
  description "indicates the type of compensation code:                    " &
              "   'I' - Income                                             " &
              "   'E' - Expense                                            " &
              "   'D' - Deduction                                          " &
              "   'P' - Payment: no affect on earnings, deductions,        " &
              "                  or eft payments. simply records what was  " &
              "                  paid to the physician automatically by the" &
              "                  system based upon potential payment.      " &
              "   'M' - Manual payment: affects earnings, no effect on     " &
              "                         deductions or eft payments.        " &
              "   'A' - Advance payment: affects eft payments but no affect" &
              "                          on deductions or earnings.        " &
              "   'C' - Ceiling calculation performed by the computer      "
 
