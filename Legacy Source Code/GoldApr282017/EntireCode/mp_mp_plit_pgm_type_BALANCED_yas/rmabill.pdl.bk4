  input scale  0                                                             &
  significance 1                                                             &
  leading sign " "                                                           &
  values 10 to 49,                                                           &
         60 to 89,                                                           &
         0 to 9,                                                             &
         50 to 59,                                                           &
         90 to 99                                                            &
  default item datatype zoned unsigned size 2                                &
  help        "10 - 49 before / 60 - 89 after ceiling calculations."         &
  description "this sequence number dictates the order in which the        " &
              "compensation transactions are processed within the physician" &
              "earnings calculation process.                               " &
              "                                                            " &
              "00 - 09 : reserved for computer generated transactions.     " &
              "                                                            " &
              "10 - 49 : for user generated transactions to be processed   " &
              "          before the ceiling calculations.                  " &
              "                                                            " &
              "50 - 59 : reserved for computer generated ceiling transact. " &
              "                                                            " &
              "60 - 89 : for user generated transactions to be processed   " &
              "          after the ceiling calculations.                   " &
              "                                                            " &
              "90 - 99 : reserved for computer generated payment transact. "
 
