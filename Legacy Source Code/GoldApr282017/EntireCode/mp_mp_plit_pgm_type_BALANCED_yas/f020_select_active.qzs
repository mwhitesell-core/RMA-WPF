select if      doc-ohip-nbr <> 0        &
          and (   doc-date-fac-start  = 0000000                         &
               or doc-date-fac-start < sysdate                          &
              )                                                         &
          and (  doc-date-fac-term   = 00000000                         &
               or doc-date-fac-term >= sysdate                          &
              )

