cancel clear

set rep nolimit
access f119-doctor-ytd
sel if comp-code = 'TOTINC' and amt-mtd <> 0
set subfile name savef119_totinc keep
rep sum doc-nbr amt-mtd
go
access *savef119_totinc		&
link (doc-nbr, 'PAYEFT'	)	&
 to  doc-nbr, comp-code of f119-doctor-ytd opt
sel if not record f119-doctor-ytd exists or amt-mtd of f119-doctor-ytd  = 0
set rep dev disc name f119_no_payeft
rep doc-nbr amt-mtd
go
