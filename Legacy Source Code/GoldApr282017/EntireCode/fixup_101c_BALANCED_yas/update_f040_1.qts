      item      fee-prev-add-on-cd(5)     &
          final fee-curr-add-on-cd(5)      
      item      fee-prev-add-on-cd(6)     &
          final fee-curr-add-on-cd(6)    
      item      fee-prev-add-on-cd(7)     &
          final fee-curr-add-on-cd(7)      
      item      fee-prev-add-on-cd(8)     &
          final fee-curr-add-on-cd(8)     
      item      fee-prev-add-on-cd(9)     &
          final fee-curr-add-on-cd(9)      
      item      fee-prev-add-on-cd(10)    &
          final fee-curr-add-on-cd(10)    
