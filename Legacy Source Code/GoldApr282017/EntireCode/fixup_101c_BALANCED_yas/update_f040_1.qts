                  and  fee-prev-add-on-cd(1)  = " "    	&
		)  						&
            or  (      fee-curr-add-on-cd(2) ne " "  & 
                  and  fee-prev-add-on-cd(2)  = " "    )  &
            or  (      fee-curr-add-on-cd(3) ne " "  & 
                  and  fee-prev-add-on-cd(3)  = " "    )  &
            or  (      fee-curr-add-on-cd(4) ne " "  & 
                  and  fee-prev-add-on-cd(4)  = " "    )  &
            or  (      fee-curr-add-on-cd(5) ne " "  & 
                  and  fee-prev-add-on-cd(5)  = " "    )  &
            or  (      fee-curr-add-on-cd(6) ne " "  & 
                  and  fee-prev-add-on-cd(6)  = " "    )  &
            or  (      fee-curr-add-on-cd(7) ne " "  & 
                  and  fee-prev-add-on-cd(7)  = " "    )  &
            or  (      fee-curr-add-on-cd(8) ne " "  & 
                  and  fee-prev-add-on-cd(8)  = " "    )  &
            or  (      fee-curr-add-on-cd(9) ne " "  & 
                  and  fee-prev-add-on-cd(9)  = " "    )  &
            or  (      fee-curr-add-on-cd(10)ne " "  & 
                  and  fee-prev-add-on-cd(10) = " "    )  &
           )       

output   f040-oma-fee-mstr update
