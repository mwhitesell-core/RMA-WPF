cancel clear
set process nolimit
access f010-pat-mstr
select if PAT-BIRTH-DATE-YY of f010-pat-mstr > 2000
output f010-pat-mstr alias f010-update update

 item PAT-BIRTH-DATE-YY   of f010-update		&
     = (PAT-BIRTH-DATE-YY of f010-pat-mstr  - 2000) 	&
     if PAT-BIRTH-DATE-YY of f010-pat-mstr  > 2000	&
  else (PAT-BIRTH-DATE-YY of f010-pat-mstr  - 4000) 	&
     if PAT-BIRTH-DATE-YY of f010-pat-mstr  > 4000

subfile f010_fix keep include f010-pat-mstr

