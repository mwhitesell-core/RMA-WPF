cancel clear
set process nolimit

access fix-claims

def x-claim-surname char*6 = CLMHDR-PAT-ACRONYM[1:6]
def x-claim-first char*3 = CLMHDR-PAT-ACRONYM[7:3]
def x-pat-surname char*6 = PAT-ACRONYM [1:6]
def x-pat-given char*3= PAT-ACRONYM[7:3]
select if  (    x-claim-surname <> x-pat-surname	&
            and x-claim-first   =  x-pat-given		&
	   )						&
        or (    x-claim-surname =  x-pat-surname	&
            and x-claim-first   <> x-pat-given		&
	   )
output fix-claims delete
