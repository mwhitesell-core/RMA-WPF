cancel clear
set rep nolimit
access f002-claims-mstr  &
 link clmhdr-pat-ohip-id-or-chart to key-pat-mstr of f010-pat-mstr

choose key-clm-type 'B', key-clm-batch-nbr, key-clm-claim-nbr, key-clm-serv-code '00000', key-clm-adj-nbr '0'
sel if clmhdr-tape-submit-ind = 'H' &
   and clmhdr-elig-error = 'VH8' 
;   and clmhdr-date-sys >= '20140516'
set subfile name checkf002_hold keep
rep summ key-clm-batch-nbr key-clm-claim-nbr key-pat-mstr clmhdr-date-sys pat-mess-code
go
