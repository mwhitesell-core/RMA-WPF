cancel clear
set rep nolimit
access f110-compensation
choose doc-nbr
sel if comp-code = 'TOTINC'
sort on doc-nbr
set subfile name savef110 keep at doc-nbr
rep summ doc-nbr comp-code amt-gross subt amt-net subt
go
access *savef110 link to f119-doctor-ytd opt
sel if not record f119-doctor-ytd exists
set rep dev disc name checkf110f119_no_totinc
set rep page length 60
set formfeed
rep doc-nbr comp-code amt-gross amt-net
go
