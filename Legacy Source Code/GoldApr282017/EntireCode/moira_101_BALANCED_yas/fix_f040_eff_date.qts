cancel clear
set process nolimit
set lock record update

access f040-oma-fee-mstr
sel if fee-icc-code = 'CP0000'	&
    or fee-icc-code = 'SP0440'	&
    or fee-icc-code = 'SP0000'	&
    or fee-icc-code = 'DT0110'	
subfile savef040_eff_date keep include f040-oma-fee-mstr
output f040-oma-fee-mstr update on errors report
   item fee-effective-date final 20091001
