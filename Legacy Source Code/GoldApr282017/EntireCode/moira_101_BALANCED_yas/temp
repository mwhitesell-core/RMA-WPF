PROCEDURE PATH
   BEGIN
     REQUEST CLMHDR-DOC-OHIP-NBR OF F002-SUSPEND-HDR
     IF PROMPTOK
       THEN REQUEST CLMHDR-ACCOUNTING-NBR OF F002-SUSPEND-HDR
     IF PROMPTOK
       THEN LET PATH = 1
     IF PATH = 0
       THEN BEGIN
         REQUEST CLMHDR-DOC-OHIP-NBR OF F002-SUSPEND-HDR
         IF PROMPTOK
           THEN LET PATH = 2
         END
     IF PATH = 0
       THEN BEGIN
         LET PATH = 3
         END
     END
 PROCEDURE FIND
   BEGIN
     IF PATH = 1
       THEN GET F002-SUSPEND-HDR VIA CLMHDR-DOC-OHIP-NBR , &
           CLMHDR-ACCOUNTING-NBR
     IF PATH = 2
       THEN GET F002-SUSPEND-HDR VIA CLMHDR-DOC-OHIP-NBR
     IF PATH = 3
       THEN GET F002-SUSPEND-HDR SEQUENTIAL
     END


