; link to clmdtl records to get the clmdtl amount 
cancel clear
access *storef002hdr 		&
    link ('B', key-clm-batch-nbr) to key-clm-type, key-clm-batch-nbr of f002-claims-mstr      
sel if clmdtl-oma-cd <> '0000' and clmdtl-oma-cd <> 'ZZZZ'
sorted on key-clm-batch-nbr
set subfile name storef002dtl keep at key-clm-batch-nbr
rep sum key-clm-batch-nbr clmhdr-tot-claim-ar-ohip  clmdtl-fee-ohip subt reset at key-clm-batch-nbr  &
xcount 
set rep nolimit
go

