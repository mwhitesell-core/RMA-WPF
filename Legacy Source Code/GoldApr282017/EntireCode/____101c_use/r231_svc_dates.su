


AA2-ACPT-SVC-DATES.

    ACCEPT SCR-SVC-DATE-FROM-YY.

    IF WS-SVC-DATE-FROM-YY < 32
    THEN
	MOVE 8				TO ERR-IND
	PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	GO TO AA2-ACPT-SVC-DATES.
*  (ELSE)
*   ENDIF

AA2-10-ACPT-MM.

    ACCEPT SCR-SVC-DATE-FROM-MM.

    IF WS-SVC-DATE-FROM-MM   <  1
			   OR > 12
    THEN
	MOVE 9				TO ERR-IND
	PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	GO TO AA2-10-ACPT-MM.
*   (ELSE)
*   ENDIF

AA2-20-ACPT-DD.

    ACCEPT SCR-SVC-DATE-FROM-DD.

    IF WS-SVC-DATE-FROM-DD < 1
    THEN
	MOVE 10				TO ERR-IND
	PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	GO TO AA2-20-ACPT-DD.
*   (ELSE)
*   ENDIF

    IF WS-SVC-DATE-FROM-MM = 2
    THEN
	IF WS-SVC-DATE-FROM-DD > 29
	THEN
	    MOVE 10			TO ERR-IND
	    PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	    GO TO AA2-20-ACPT-DD
	ELSE
	    GO TO AA2-30-ACPT-SVC-DATE-TO.
*	ENDIF
*   (ELSE)
*   ENDIF

    IF WS-SVC-DATE-FROM-MM =    4
			    OR  6
			    OR  9
			    OR 11
    THEN
	IF WS-SVC-DATE-FROM-DD > 30
	THEN
	    MOVE 10			TO ERR-IND
	    PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	    GO TO AA2-20-ACPT-DD
	ELSE
	    NEXT SENTENCE
*	ENDIF
    ELSE
	IF WS-SVC-DATE-FROM-DD > 31
	THEN
	    MOVE 10			TO ERR-IND
	    PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	    GO TO AA2-20-ACPT-DD.
*	(ELSE)
*	ENDIF
*   ENDIF

AA2-30-ACPT-SVC-DATE-TO.

    ACCEPT SCR-SVC-DATE-TO-YY.

    IF WS-SVC-DATE-TO-YY < 32
    THEN
	MOVE 8				TO ERR-IND
	PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	GO TO AA2-30-ACPT-SVC-DATE-TO.
*  (ELSE)
*   ENDIF

    IF WS-SVC-DATE-TO-YY < WS-SVC-DATE-FROM-YY
    THEN
	MOVE 11				TO ERR-IND
	PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	GO TO AA2-30-ACPT-SVC-DATE-TO.
*   (ELSE)
*   ENDIF

AA2-40-ACPT-MM.

    ACCEPT SCR-SVC-DATE-TO-MM.

    IF WS-SVC-DATE-TO-MM   <  1
			OR > 12
    THEN
	MOVE 9				TO ERR-IND
	PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	GO TO AA2-40-ACPT-MM.
*   (ELSE)
*   ENDIF

AA2-50-ACPT-DD.

    ACCEPT SCR-SVC-DATE-TO-DD.

    IF WS-SVC-DATE-TO-DD < 1
    THEN
	MOVE 10				TO ERR-IND
	PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	GO TO AA2-50-ACPT-DD.
*   (ELSE)
*   ENDIF

    IF WS-SVC-DATE-TO-MM = 2
    THEN
	IF WS-SVC-DATE-TO-DD > 29
	THEN
	    MOVE 10			TO ERR-IND
	    PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	    GO TO AA2-50-ACPT-DD
	ELSE
	    GO TO AA2-60-CHECK-FROM-TO.
*	ENDIF
*   (ELSE)
*   ENDIF

    IF WS-SVC-DATE-TO-MM =    4
			  OR  6
			  OR  9
			  OR 11
    THEN
	IF WS-SVC-DATE-TO-DD > 30
	THEN
	    MOVE 10			TO ERR-IND
	    PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	    GO TO AA2-50-ACPT-DD
	ELSE
	    NEXT SENTENCE
*	ENDIF
    ELSE
	IF WS-SVC-DATE-TO-DD > 31
	THEN
	    MOVE 10			TO ERR-IND
	    PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	    GO TO AA2-50-ACPT-DD.
*	(ELSE)
*	ENDIF
*   ENDIF

AA2-60-CHECK-FROM-TO.

    IF WS-SVC-DATE-TO < WS-SVC-DATE-FROM
    THEN
	MOVE 12				TO ERR-IND
	PERFORM ZA0-COMMON-ERROR	THRU ZA0-99-EXIT
	GO TO AA2-ACPT-SVC-DATES.
*   (ELSE)
*   ENDIF

AA2-99-EXIT.
    EXIT.
