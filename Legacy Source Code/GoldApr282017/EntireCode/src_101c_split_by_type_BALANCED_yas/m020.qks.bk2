procedure designer exp
; EXPort function to copy bank info to another system.
; file always contains id of user and the info type as first 2 fields
begin
  if doc-name = " " 
  then
    let x-doc-name = "|"
  else 
    let x-doc-name =  truncate(lj(doc-name)) 

