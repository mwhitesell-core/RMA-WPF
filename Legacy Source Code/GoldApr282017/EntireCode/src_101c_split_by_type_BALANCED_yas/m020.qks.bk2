		+ '"' + truncate(lj(x-doc-inits)) + '"'		&
		+ ' '						&
		+ ascii(doc-ohip-nbr,6)				& 
		+ ' '						&
		+ ascii(doc-sin-nbr,9)				&
		+ ' '						&
		+ '"' + truncate(lj(x-doc-sub-specialty)) + '"'&
		+ ' '						&
