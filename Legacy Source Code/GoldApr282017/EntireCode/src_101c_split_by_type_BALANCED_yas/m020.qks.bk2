  then
    let x-doc-addr-office-pc = "|"
  else
    let x-doc-addr-office-pc = truncate(lj(doc-addr-office-pc)) 

  if doc-bank-acct = " "
  then
    let x-doc-bank-acct = "|"
  else
    let x-doc-bank-acct = truncate(lj(doc-bank-acct)) 
  let comline = '$cmd/batch_copy_bank_info_out '		&	
		+ truncate(lj(logonid))				&
		+ ' '						&
		+ 'f020'					&
		+ ' '						&
