; as per Theglas request print reject code HM claim numbers 
; August 10, 2006

set rep nolimit
set page length 0
set rep dev disc name r030_hm

access u030-tape-145-file
select if RAT-145-EXPLAN-CD = "HM"
rep RAT-145-GROUP-NBR  RAT-145-ACCOUNT-NBR RAT-145-EXPLAN-CD

page heading &
skip 1 "Claims rejected with RAT reason code HM"  &
skip 2

build $obj/r030_hm
