; CLAIMS MSTR RECORD CONVERSION FOR 'B' ADJUSTMENT - SMS 97.1

access f002-claims-mstr
set process limit 300000
select if clmhdr-oma-suff-adj = "000000"  &
     and  clmhdr-batch-type ="A" and clmhdr-adj-cd = "B"   &
     and  clmhdr-adj-cd-sub-type = "0"
output f002-claims-mstr update
	item clmhdr-adj-cd-sub-type final "M"
build $pb_obj/u904

  
