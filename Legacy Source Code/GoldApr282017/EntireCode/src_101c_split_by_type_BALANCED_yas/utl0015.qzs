cancel clear
set rep nolimit
set rep dev disc name utl0015
set page widt 180
set rep nohead
set page length 0

access *f020_active_with_clinic	&
	link doc-nbr to f020-doctor-mstr

sort on doc-clinic-nbr	&
	on doc-ohip-nbr &
	on doc-name     &
        on doc-inits

footing at doc-clinic-nbr skip 2

rep doc-clinic-nbr &  ;print at doc-clinic-nbr	&
	doc-nbr doc-dept doc-name doc-inits     &
        doc-ohip-nbr doc-date-fac-start doc-date-fac-term
