cancel clear
set rep nolimit
set rep dev printer

access f190-comp-codes
 sort on   process-seq on comp-code

initial heading                 &
	tab 01 "DUMP_F190_2"  	&
        tab 15 sysdate          &
	tab 35 "COMPENSATION CODES SORTED BY"	&
 skip 1 tab 35 "    ON LINE PROCESS SEQ"   	&
 skip 2

 rep process-seq comp-code comp-type comp-code-group desc-long
go
  
