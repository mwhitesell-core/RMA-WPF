        clmdtl-amt-tech-billed  pic "^^^,^^^.^^"        &
        clmdtl-fee-oma  pic "^^^,^^^.^^"        	&
        clmdtl-fee-ohip pic "^^^,^^^.^^"

build $obj/dump_tech2

