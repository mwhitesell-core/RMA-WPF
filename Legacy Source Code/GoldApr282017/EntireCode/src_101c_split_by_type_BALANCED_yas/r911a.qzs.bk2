;
; Produce a list of all assigned doctor numbers
;
cancel clear
set report device disc name r911a
set rep nolimit

access f021-avail-doctor-mstr
select if doc-status = 'T'

page heading    &
tab 1 "R911A"   &
tab 10 sysdate  &
tab 25 "LIST OF ASSIGNED DOCTOR NUMBERS AFTER U023 AND R023" &
skip 1								&
tab 25 "INVESTIGATE FOLLOWING DOCTORS WHICH WAS NOT ADDED TO DR. MSTR" &
skip 2

rep all

build $pb_obj/r911a
  
