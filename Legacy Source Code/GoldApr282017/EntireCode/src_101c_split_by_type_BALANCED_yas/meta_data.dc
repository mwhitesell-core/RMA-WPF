; (this file is used when changes are made to claims that require the update
;  of the f050-doc-rev-mstr and f050-doc-rev-mstr-history files. The amounts
;  are subtracted from the 'orig' record and added to the 'new' record)
file f086b-orig-new-revenue-recs                                             &
  organization sequential                                                    &
  create                                                                     &
  open f086b_orig_new_revenue_recs
 
  record f086b-orig-new-revenue-recs
;         (ped taken from claim and used to figure out which f050 history
;	   records need to be altered)
    item clmhdr-date-period-end
;        (numeric values below reflect net difference of update - the mtd
;         values are affected only if the claim's PED is the same as the
;	  current PED in the constants master for clinic). The batch control
;	  file's status indicates if the claim has updated revenue)
    item docrev-mtd-in-rec
    item docrev-mtd-in-svc
    item docrev-mtd-out-rec
    item docrev-mtd-out-svc
    item docrev-ytd-in-rec
    item docrev-ytd-in-svc
    item docrev-ytd-out-rec
    item docrev-ytd-out-svc

;   (original location)
    item docrev-key-orig 
    begin structure
      item docrev-clinic-1-2-orig
      item docrev-dept-orig
      item docrev-doc-nbr-orig
      item docrev-location-orig
      item docrev-oma-cd-orig
      begin structure
        item docrev-oma-code-orig
        item docrev-oma-suff-orig
      end structure
    end structure

;   (new location)
    item docrev-key
    begin structure
      item docrev-clinic-1-2
      item docrev-dept
      item docrev-doc-nbr
      item docrev-location
      item docrev-oma-cd
      begin structure
        item docrev-oma-code
        item docrev-oma-suff
        item docrev-adj-cd-sub-type                                          &
          redefines item docrev-oma-suff
      end structure
    end structure

