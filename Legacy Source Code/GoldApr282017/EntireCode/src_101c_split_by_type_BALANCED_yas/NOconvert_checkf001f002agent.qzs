access f001-batch-control-file
;choose batctrl-batch-nbr '78P51003', '78P85005', '78R03002', '78R03016'        
choose batctrl-batch-nbr '22X93963' 
set subfile name self001 keep
rep summ all
set rep nolimit
go
access *self001 link ('B', batctrl-batch-nbr)   &
to key-clm-type, key-clm-batch-nbr of f002-claims-mstr
sel if clmdtl-oma-cd = '0000'
set subfile name self002hdragent keep 
rep summ batctrl-batch-nbr batctrl-agent-cd batctrl-nbr-claims-in-batch clmhdr-agent-cd &
key-clm-claim-nbr clmhdr-claim-id  
go

access *self002hdragent link ('B', batctrl-batch-nbr, key-clm-claim-nbr)   &
to key-clm-type, key-clm-batch-nbr, key-clm-claim-nbr of f002-claims-mstr
sel if clmdtl-oma-cd <> '0000'
set rep page width 132
set rep nolimit
;set subfile name self002dtlagent keep 
;rep summ batctrl-batch-nbr batctrl-agent-cd batctrl-nbr-claims-in-batch clmhdr-agent-cd &
rep batctrl-batch-nbr batctrl-agent-cd batctrl-nbr-claims-in-batch clmhdr-agent-cd &
key-clm-claim-nbr clmhdr-claim-id  clmdtl-id clmdtl-agent-cd
go
