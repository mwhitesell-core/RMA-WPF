; BATCH CONTROL RECORD CONVERSION FOR 'B' ADJUSTMENT - SMS 97.1

access f001-batch-control-file
sel if batctrl-batch-type = "A" and batctrl-adj-cd = "B" and &
       batctrl-adj-cd-sub-type = "0"
set process limit 100000
output f001-batch-control-file update
	item batctrl-adj-cd-sub-type final "M"
build $pb_obj/u903
  
