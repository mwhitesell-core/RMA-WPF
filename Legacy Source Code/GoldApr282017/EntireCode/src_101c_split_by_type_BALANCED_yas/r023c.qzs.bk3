select if doc-status = 'T'

  set	report	device disc					&
		name r023b					&
		nolimit						&
		spacing 1					&
		copies 1					&
	blanks							&
	close							&
	noformfeed						&
	head							&
	nosubfile 						&
	page	length 60					&
		width 132					&
		number 1					&
		images 1


