	skip 1	tab 1	"R023B"					&
		tab 50	"LIST OF UNDELETED DOCTORS"		&
		tab 96	"RUN DATE: "				&
		tab 108	sysdate					&
				format yymmdd			&
	skip 2	tab 1	"DOC #"					&
		tab 8	"NAME"					&

	skip 1



report                						&
	skip 1	tab 1	doc-nbr					&
		tab 8   doc-full-name

