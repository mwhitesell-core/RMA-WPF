cancel clear
set rep nolimit
set rep dev printer

access f190-comp-codes
 sort                  on comp-code

initial heading                 &
	tab 01 "DUMP_F190_1"  	&
        tab 15 sysdate          &
	tab 35 "COMPENSATION CODES SORTED BY"	&
 skip 1 tab 35 "          ALPHA"           	&
 skip 2

 rep comp-code comp-type process-seq desc-long desc-short
go
  
