;			  technical & professional amt billed


request calc_tech_prof on calculation errors report on edit errors report

access *u030_srt_adj link x-adj-serv-code to fee-oma-cd of f040-oma-fee-mstr &
 link   'B', nconvert(part-hdr-claim-id[1:9]),        &
        nconvert(part-hdr-claim-id[10:2]), '00000', '0'   &
    to   key-clm-type, key-clm-batch-nbr,             &
        key-clm-claim-nbr, key-clm-serv-code, key-clm-adj-nbr &
         of f002-claims-mstr 

def x-prof-ind char*1 = 'Y'				&
	 if fee-icc-sec = 'DR' 		    		&
	 or fee-icc-sec = 'DU'				&
	 or fee-icc-sec = 'NM'				&
	 or fee-icc-sec = 'PF'				&
	 or fee-tech-ind = 'N'  

def x-tech-ind char*1 = 'Y'				&
	 if fee-icc-sec = 'DR' 		    		&
	 or fee-icc-sec = 'DU'				&
	 or fee-icc-sec = 'NM'				&
	 or fee-icc-sec = 'PF'				&
	 or fee-tech-ind = 'Y'  

def x-prof-prev   = fee-prev-h-fee-2        		&
	 if fee-icc-sec = 'DR' 		    		&
	 or fee-icc-sec = 'DU'				&
	 or fee-icc-sec = 'NM'				&
	 or fee-icc-sec = 'PF'				&
	else fee-prev-h-fee-1 

def x-prof-incr = fee-prev-h-fee-2 * .0195		&
	 if fee-icc-sec = 'DR' 		    		&
	 or fee-icc-sec = 'DU'				&
	 or fee-icc-sec = 'NM'				&
	 or fee-icc-sec = 'PF'				&
	else fee-prev-h-fee-1 * .0195

def x-prof-curr   = fee-curr-h-fee-2        		&
	 if fee-icc-sec = 'DR' 		    		&
	 or fee-icc-sec = 'DU'				&
	 or fee-icc-sec = 'NM'				&
	 or fee-icc-sec = 'PF'				&
	else fee-curr-h-fee-1 

; 2000/09/14 - MC
;def x-prof-stage-1 = round(x-prof-incr,0,near) 			
def x-prof-stage-1 zoned*5 signed = round(x-prof-incr/10)       	
;;
def x-prof-ascii   char*5 = ascii(x-prof-stage-1,5)
def x-prof-stage-2 = nconvert(x-prof-ascii[1:4] + '0')	&
	if x-prof-ascii[5:1] >= '0' and x-prof-ascii[5:1] <= '2' &
	else nconvert(x-prof-ascii[1:4] + '5')		&
	if x-prof-ascii[5:1] >= '3' and x-prof-ascii[5:1] <= '7' &
	else nconvert(x-prof-ascii[1:4] + '0')+ 10		&
	if x-prof-ascii[5:1] >= '8' and x-prof-ascii[5:1] <= '9' 

def x-prof = x-prof-stage-2      			&
	 if    x-prof-ind = 'Y'   			&
	   and clmdtl-sv-date >= '20000401'		&
	   and clmhdr-date-sys < '20000828'		&
	else x-prof-prev				&
	 if    x-tech-ind = 'Y'				&
	   and x-prof-ind = 'Y'				&
	   and part-dtl-explan-cd = '35' 		&
	   and (clmdtl-sv-date <= '20000331'  		&
	         or  (clmdtl-sv-date >= '20000401'	&
		 and  clmhdr-date-sys < '20000828'))	&
	else x-prof-curr				& 
	 if    x-tech-ind = 'Y'				&
	   and x-prof-ind = 'Y'				&
	   and part-dtl-explan-cd = '35' 		&
	   and clmdtl-sv-date  >= '20000401'		&
   	   and clmhdr-date-sys >= '20000828'	


; 2000/09/14 - MC
;def x-tech-decr integer signed size 4 = round(fee-curr-h-fee-1 * -.07)	&
def x-tech-decr zoned*6 signed = round(fee-curr-h-fee-1 * -.07 / 10)	&
;;
	if clmdtl-sv-date >= '20000401'			&
; 2000/09/14 - MC
; else  round(fee-prev-h-fee-1 * -.07)		&
  else  round(fee-prev-h-fee-1 * -.07 / 10)	&
;;
	if clmdtl-sv-date <= '20000331'			
	
; 2000/09/14 - MC
;def x-tech integer signed  size 4 = fee-curr-h-fee-1    &
def x-tech zoned*6 signed         = fee-curr-h-fee-1    &
;;
	if    x-tech-ind = 'Y' 				&
	  and x-prof-ind = 'Y'				&
	  and part-dtl-explan-cd = '35'			&
	  and clmdtl-sv-date >= '20000401'		&
	else fee-prev-h-fee-1				&
	if    x-tech-ind = 'Y' 				&
	  and x-prof-ind = 'Y'				&
	  and part-dtl-explan-cd = '35'			&
	  and clmdtl-sv-date <= '20000331'		&
	else x-tech-decr if x-tech-ind = 'Y'  

def x-tot-adj = x-tech + x-prof
def x-nbr-serv = round(x-bal-diff / x-tot-adj)

def x-tech-amt = x-bal-diff if fee-tech-ind = 'Y'	&
 	 		  or (x-prof = 0		&
			 and  clmdtl-sv-date <= '20000331') &
	else (x-bal-diff - (x-nbr-serv * x-prof))       & 
			    if x-prof <> 0		& 
			   and x-tech-ind = 'Y' 	&
                           and x-prof-ind = 'Y' ;&
;		      	   and clmdtl-sv-date >= '20000401'

def x-prof-amt = x-bal-diff if x-tech-ind = ' '   	& 
			   and x-prof-ind = 'Y'		& 
	                   and clmdtl-sv-date >= '20000401' &
	else x-nbr-serv * x-prof  if x-prof <> 0	&
			   and x-tech-ind = 'Y' 	&
                           and x-prof-ind = 'Y' ;	&
;		      	   and clmdtl-sv-date >= '20000401'

subfile u030mc keep include	&
	u030_srt_adj,		&
	fee-tech-ind,		&
	fee-icc-sec, 		&
	fee-curr-h-fee-1,       &
	fee-curr-h-fee-2,	&
	x-tech, x-prof,		&
	x-tech-ind, x-prof-ind, &
 	x-tech-amt, x-prof-amt


