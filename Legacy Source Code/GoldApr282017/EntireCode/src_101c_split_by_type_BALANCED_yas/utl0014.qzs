; program utl0014
; purpose: dump out any patients with non-blank country 
;


cancel clear
set rep dev disc name utl0014
set page length 0 width 132
set rep nolimit

access f010-pat-mstr

select if     pat-country <> " "	&
	  and pat-country <> "CAN"	&
	  and pat-country <> "CANADA"	

rep pat-given-name pat-surname subscr-addr1 subscr-addr2 subscr-addr3 pat-country

build $obj/utl0014

