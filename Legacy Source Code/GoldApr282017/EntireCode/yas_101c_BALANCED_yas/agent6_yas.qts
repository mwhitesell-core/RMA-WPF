;doc: agent6.qts
;doc: report number of bill directs for 2001 and 2002
;     compare number of billdirects generated from march 2000 feb 2001
;     and march 2001 feb 2002


;For: Mary Brownridge


;Date           Who             Description
;2002/04/03     Yasemin         Original

set lock file update
set process nolimit

request one on calculation errors report	&
	    on edit errors report

access f002-claims-mstr

;choose all regular 'C'laim batch type
choose  key-p-clm-type 'P'

select f002-claims-mstr if                             &
        (      clmhdr-date-period-end >=  20000301     &
           and clmhdr-date-period-end <=  20020331)    &
     and       clmhdr-agent-cd = 6

def x-yyyy num*4 = nconvert(ascii(clmhdr-date-period-end[1:4]))
def x-mm   num*2 = nconvert(ascii(clmhdr-date-period-end[5:6]))

sort on clmhdr-agent-cd

temp x-2001 num*7
item x-2001  = x-2001 + clmhdr-agent-cd             &
               if   (x-yyyy = 2000 and x-mm >= 03)  &
                 or (x-yyyy = 2001 and x-mm <= 02)

temp x-2002 num*7
item x-2002  = x-2002 + clmhdr-agent-cd   &
               if   (x-yyyy = 2001 and x-mm >= 03)  &
                 or (x-yyyy = 2002 and x-mm <= 02)

subfile agent6 keep include   &
x-2001,                       &
x-2002


build $pb_obj/agent6




