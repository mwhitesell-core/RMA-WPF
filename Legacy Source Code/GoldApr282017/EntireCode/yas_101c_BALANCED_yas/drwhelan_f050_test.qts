;for Dr. Whelan 03-G30 clinic 33  report dollar amount for S434A, S435A and S436A from January 2008 to December 2010
;    Jan 2008 is EP 200707
;    Dec 2010 is EP 201006

cancel clear
set process nolimit
set lock file update 

access f050-doc-revenue-mstr-history                            &
 link docrev-doc-nbr to doc-nbr     of f020-doctor-mstr  opt	&
 link docrev-oma-code  to fee-oma-cd  of f040-oma-fee-mstr opt

;choose ep-yr 2007 to 2010, iconst-date-period-end 20080101 to 20101231, docrev-key '3303G30@'
choose ep-yr 2012, iconst-date-period-end 20120701 to 20130331, docrev-key '3303G30@'

select if  				   &
              (   docrev-oma-cd = "S434A"  &
               or docrev-oma-cd = "S435A"  & 
               or docrev-oma-cd = "S436A") 


temp x-total zoned*9 signed
item x-total  = x-total + (docrev-mtd-in-rec + docrev-mtd-out-rec)


def comma cha*1 = "~"
def x-num-cr integer unsigned size 2 = 13
def x-cr        char*2 = char(x-num-cr)

subfile f050whelan keep at final portable include		&
docrev-doc-nbr       , &
comma                , &
x-total              , &
x-cr                

build $pb_obj/f050whelan

