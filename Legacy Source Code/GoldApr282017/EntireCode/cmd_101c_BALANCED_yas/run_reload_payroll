# RUN_RELOAD_PAYROLL
# RMA Physician Payroll QTP $obj/greload.
# This script runs the QTP programs that $obj/greload the files 
# in test from the subfiles that were created from production.
# Created by S. Bachmann on Jan. 27, 1999
# 1999/may/03 B.E. added delete/cr-create of data files before reload

echo Physician Payroll QTP $obj/greload.   
echo "Recreating empty PH only files"
cd $pb_data
pwd
rm f1*
qutil << QUTIL_EXIT
create file f110-compensation
create file f110-compensation-history
create file f112-pycdceilings
create file f112-pycdceilings-history
create file f113-default-comp
create file f113-default-comp-history
create file f119-doctor-ytd-history
create file f190-comp-codes
create file f191-earnings-period
create file f198-user-defined-totals
create file f199-user-defined-fields
QUTIL_EXIT
echo "Recreating empty PH/cobol files"
rm f020_doctor_mstr*
rm f020_doctor_extra*
rm f050_doc_revenue_mstr*
rm f090_constants_mstr*

cp /alpha/rmabill/rmabill102/data/f020_doctor_extra.dat f020_doctor_extra.dat
cp /alpha/rmabill/rmabill102/data/f020_doctor_extra.idx f020_doctor_extra.idx
cp /alpha/rmabill/rmabill102/data/f050_doc_revenue_mstr f050_doc_revenue_mstr
cp /alpha/rmabill/rmabill102/data/f050_doc_revenue_mstr.idx f050_doc_revenue_mstr.idx
cp /alpha/rmabill/rmabill102/data/f119_doctor_ytd.dat      f119_doctor_ytd.dat
cp /alpha/rmabill/rmabill102/data/f119_doctor_ytd.idx      f119_doctor_ytd.idx

. ./createfiles.com

echo "Reloading files ..."
qtp << QTP_EXIT
execute $obj/relof020  
;execute $obj/relof020hst  
execute $obj/relof090_1  
execute $obj/relof090_2  
execute $obj/relof090_3  
execute $obj/relof090_4  
execute $obj/relof090_5  
execute $obj/relof090_6  
execute $obj/relof090_iconst  
execute $obj/relof110  
execute $obj/relof110hst  
execute $obj/relof112  
execute $obj/relof112hst  
execute $obj/relof113  
execute $obj/relof113hst  
execute $obj/relof119hst
execute $obj/relof190  
execute $obj/relof191  
execute $obj/relof199  
QTP_EXIT
