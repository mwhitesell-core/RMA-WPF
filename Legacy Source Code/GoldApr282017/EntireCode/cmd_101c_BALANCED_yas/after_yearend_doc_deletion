cd $pb_data

rm $pb_data/f022_deleted_doc_mstr.dat
qutil 1>/dev/null 2>&1 << QUTIL_EXIT
create file f022-deleted-doc-mstr
QUTIL_EXIT

bcheck $pb_data/tmp_counters
qutil 1>/dev/null 2>&1 << QUTIL_EXIT
create file tmp-counters
QUTIL_EXIT

cd $application_upl

