cd $application_production/60
echo '********************************************************************'
echo  Clinic 60 Monthend Reports
echo
echo  Make sure you have printed all your extra copies of Reports
echo  
echo  r004tp r005tp r006tp r011 r012tp r013tp r015tp r051ca r051cb r070
