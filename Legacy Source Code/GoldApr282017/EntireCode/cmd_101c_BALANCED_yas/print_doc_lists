echo   "PRINT_DOC_LISTS"
echo    
echo 
echo               ENTER TYPE OF LIST TO PRINT
echo 
echo                     1 - PAYROLL LIST
echo                     2 - BILLING LIST
echo                     3 - DOCTOR  DUMP
echo 
echo "Enter Type ?"
read string
echo 
echo 
if [ "$string" = "1" ]
 then
ls -laF r020a
    echo 
    lp -H hold r020a
else
    if [ "$string" = "2" ]
 then
ls -laF r020b
      echo 
   lp -H hold r020b
    else
 if [ "$string" = "3" ]
 then
ls -laF r020c
      echo 
           lp -H hold r020c
        else
     echo  
      echo  INVALID OPTION
            echo  
  fi
    fi
fi

echo 
echo 
echo    FINISHED....
echo 
