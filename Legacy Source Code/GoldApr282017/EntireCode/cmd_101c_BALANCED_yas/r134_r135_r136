# 2015/Apr/15	MC	r134_r135_r136   
#                 	include the parameter 'REGULAR' and 'PORTAL' to run r134 & r135 twice
#			include the run of r136.qtc/qzc at then end

cd $application_root/production

echo "Start  - `date`"

rm r134*txt  r135*txt  r136*txt  r134.sf*  r135.sf*  1>/dev/null 2>/dev/null

echo "Running r134_r135_portal - `date`"
echo

$cmd/r134_r135_portal    > r134_r135_portal.log   

echo "Rename  r134_r135_portal - `date`"
echo

echo "Rename r134 and r135 reports to portal"  >> r134_r135_portal.log
mv r134.txt  r134_portal.txt
mv r135.txt  r135_portal.txt

echo "Running r134_r135_regular - `date`"
echo

$cmd/r134_r135_regular   > r134_r135_regular.log   

echo "Running r136   - `date`"
echo

$cmd/r136                > r136.log   

echo "Finish - `date`"
