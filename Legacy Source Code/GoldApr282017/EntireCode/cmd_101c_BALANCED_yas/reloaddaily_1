echo  "reloaddaily_1"
echo  
[ $# -eq 0 ] && echo "usage: reloaddaily_1 {filename}" && exit

echo  Reload from tape created by  "backupdaily_1.cli"
echo 
echo  ----------!  f002   - claims mstr
echo  ----------!  f002   - claims extra
echo 
echo 
mt rewind
echo 

cd $pb_data

echo "Reload $1 ? (y/n)"
read string

if [ "$string" = "y" -o "$string" = "Y" ]
then
	cd $pb_data
	pwd 
	echo 
	date 
	echo  
	cpio -icvb -I/dev/rmt/1 "$1"
	echo  
	date 
fi

echo 
echo 
echo 
cd $application_upl
mt rewind
echo 
echo  Finished ...
echo 
