#!/bin/ksh
# file: run_icu_payroll_2
#  ICU Payroll Run 2
#

echo
echo Running \'run_icu_payroll_2\'
echo
echo Upon completion - verify results in log file: \'teb_icu.log\'
echo
echo Hit ENTER to start
read garbage
echo continuing ...

rm teb_icu.log 1>/dev/null 2>&1

echo "ICU Payroll Run 2 - starting - `date`" > teb_icu.log

echo "--- generate_icu_payroll ---"         >> teb_icu.log
$cmd/generate_81y2k_payroll 200102 200101     >> teb_icu.log 2>&1

echo "--- u090f (QTP RUN) ---" 		    >> teb_icu.log
qtp auto=$obj/u090f.qtc 		    >> teb_icu.log 2>&1

echo "ICU - Payroll Run 2 - ending - `date`">> teb_icu.log
