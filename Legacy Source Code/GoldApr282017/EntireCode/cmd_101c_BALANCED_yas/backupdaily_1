echo  "BACKUPDAILY_1"
echo  

echo  BACKUP OF:
echo  ----------!  f002   - claims mstr
echo  ----------!  f002   - claims extra
echo 
echo 

echo  'HIT   "NEWLINE"   TO COMMENCE BACKUP ...'
read garbage

echo 
echo  BACKUP NOW COMMENCING ...
echo 

echo  
date 
echo  

cd $pb_data
/bin/ls f002_claims_mstr*  \ 
   	f002_claims_extra* \
	> backupdaily_1.ls

cat backupdaily_1.ls | cpio -ocuvB |dd of=/dev/rmt/1
 
echo  
date 
echo  

mt -f /dev/rmt/1 rewind
