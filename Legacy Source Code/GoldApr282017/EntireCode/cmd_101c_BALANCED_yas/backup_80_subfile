echo  "backup_80_subfile"
echo  
read garbage

echo 
echo  BACKUP NOW COMMENCING ...

date 
echo  

cd  $application_root
pwd
/bin/ls                 	\
yasemin/claims_subfile_80*.sf*  > yasemin/backup_80_subfile.ls 
echo
date
cat yasemin/backup_80_subfile.ls |cpio -ocuvB > /devrmt/1
echo
date
echo "DONE!"
 
mt -f /dev/rmt/1 rewind
