quiz auto=$obj/grek_ca.qzc
echo " --- grek_cb.qzc --- "
quiz auto=$obj/grek_cb.qzc

lp grek_ca.txt
lp grek_cb.txt

lp grek_ca.txt
lp grek_cb.txt
