echo   "PRINT_DOC_LIST_AND_LABELS"
echo  
echo 
echo               ENTER TYPE OF LIST TO PRINT
echo 
echo "                   1 - PAYROLL LIST  (CANCELLED)"
echo "                   2 - BILLING LIST"
echo "                   3 - DOCTOR  DUMP  (CANCELLED)"
echo "                   4 - ADDRESS LABELS"
echo "                   5 - DOCTOR  LABELS *SIGNATURE*"
echo "                   6 - DOCTOR  LIST   *PROMP USER FOR DATES*"
echo "                   7 - LIST OF DOCTORS WITH PAY CODE 2  3  AND  4"
echo "                   8 - DOCTOR LABELS FOR TAX PURPOSES  (CANCELLED)"
echo 
echo "Enter Type ?"
read string
echo 
echo 
if [ "$string" = "1" ]
 then
    lp payrolllist.txt
else
    if [ "$string" = "2" ]
 then
        lp billinglist.txt
    else
  if [ "$string" = "3" ]
 then
            lp doctordump.txt
  else
     if [ "$string" = "4" ]
 then
                lp -H hold addrlabels.txt
          else
             if [ "$string" = "5" ]
 then
                    lp -H hold signaturelabels.txt
             else
             if [ "$string" = "6" ]
 then
                        lp doctorlist.txt
              else
                     if [ "$string" = "7" ]
 then
                            lp docpaycode.txt
                  else
                     if [ "$string" = "8" ]
 then
                                lp -H hold taxlabels.txt
                           else
                              echo  
                              echo  INVALID OPTION
                            echo  
                          fi
                      fi
                     fi
                 fi
             fi
         fi
     fi
 fi
echo 
echo 
echo    FINISHED....
echo 
