# 2010/09/24  yas Print total Adj for the month                                    

rm r950*.txt 1>/dev/null 2>&1  
rm r950*.sf* 1>/dev/null 2>&1  

quiz auto=$obj/r950a.qzc        
quiz auto=$obj/r950b.qzc        
quiz auto=$obj/r950c.qzc        
mv r950c.txt r950c_dtl.txt

quiz << quiz_exit
exec $obj/r950c nogo
rep
go
quiz_exit

mv r950c.txt r950c_tot.txt

lp r950c_tot.txt
