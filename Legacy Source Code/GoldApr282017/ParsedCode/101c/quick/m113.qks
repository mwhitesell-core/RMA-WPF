field column-nbr-doc-surname		required
field column-nbr-doc-inits		required
field column-nbr-doc-given-names	required
field column-nbr-amt			required
field column-nbr-comp-code		required

;TODO verify no 2 'column-nbr' fields are the same

build 
  
