; purpose: before adding patients to f010, ensure the ikeys are not already there
;


cancel clear
set rep  nolimit

access *u099-delete-patients			&
  link  (key-pat-mstr[2:15]			& 	
	)					&
    to	key-pat-mstr of  tmp-pat-mstr 		&
  link key-pat-mstr				&
    to key-pat-mstr  of   f010-pat-mstr opt

define x-problem-flag char*1  		&
   = "Y" if record f010-pat-mstr exists

select if x-problem-flag = "Y"

rep x-problem-flag

