;suspend_agent.qzs
;purpose: report agent <> 0 for Linda and Renee
; 
;DATE:          WHO:            MODIFICATION
;0000/07/24     yasemin         original
;
;

can clear
set rep nolimit
set rep page length 62
set rep page width 132
set rep dev disc name suspend_agent
set formfee


access f002-suspend-hdr

select if clmhdr-agent-cd <> 0

sort on clmhdr-doc-nbr  &
     on clmhdr-agent-cd

page heading                   &
skip 1  tab 1   sysdate        &
        tab 20  "suspend_agent"&
skip 2  tab 1   "Agent"        &  
        tab 10  "Doc#"         &
        tab 18  "Account#"     &   
        tab 30  "Patient Name" &
skip 1  tab 1   "====="        &
        tab 10  "===="         &
        tab 18  "========"     &   
        tab 30  "============" &
skip 2

heading at clmhdr-agent-cd      &
skip 2

report                                 &
skip 1  tab 3   clmhdr-agent-cd        &
        tab 8   clmhdr-doc-nbr         &
        tab 18  clmhdr-accounting-nbr  & 
        tab 30  clmhdr-pat-acronym     &
skip 1

build $pb_obj/suspend_agent
