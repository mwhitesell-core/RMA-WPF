   or (    doc-afp-paym-group of f020-doctor-mstr <> " "	&
       and mtd-bill <> 0                                        &
      )								&
     include docrev-doc-nbr of f050-doc-revenue-mstr,comp-code,comp-type,process-seq,factor,mtd-billing

  
